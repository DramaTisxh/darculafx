<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<MenuBar xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem mnemonicParsing="false" text="MenuItem" />
            <RadioMenuItem mnemonicParsing="false" text="RadioMenuItem" />
            <CheckMenuItem mnemonicParsing="false" text="CheckMenuItem" />
            <CustomMenuItem mnemonicParsing="false" text="CustomMenuItem">
              <content>
                <CheckBox text="Click Me" />
              </content>
            </CustomMenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Edit">
      <items>
        <MenuItem mnemonicParsing="false" text="Delete" />
      </items>
    </Menu>
    <Menu disable="true" mnemonicParsing="false" text="Help">
      <items>
        <MenuItem mnemonicParsing="false" text="About" />
      </items>
    </Menu>
  </menus>
</MenuBar>
